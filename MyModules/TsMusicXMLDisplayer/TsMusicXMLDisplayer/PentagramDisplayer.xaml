<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="TsMusicXMLDisplayer.PentagramDisplayer"
	x:Name="UserControl" Height="233" Loaded="UserControl_Loaded" SizeChanged="UserControl_SizeChanged" Width="656">

	<Grid x:Name="LayoutRoot">
		<Grid.RowDefinitions>
			<RowDefinition Height="1*"/>
			<RowDefinition Height="1*"/>
		</Grid.RowDefinitions>
		<Canvas x:Name="canvasB" Grid.Row="1"/>
		<Grid x:Name="gridA">
			<Image x:Name="imageA"/>
		</Grid>
	</Grid>
</UserControl>