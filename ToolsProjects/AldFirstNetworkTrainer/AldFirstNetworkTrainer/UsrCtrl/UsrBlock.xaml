<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="AldFirstNetworkTrainer.UsrBlock"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="480" Background="#BF0046FF">

	<Grid x:Name="LayoutRoot">
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="54.286"/>
			<RowDefinition Height="50"/>
			<RowDefinition Height="50"/>
			<RowDefinition/>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="150*"/>
			<ColumnDefinition Width="350"/>
			<ColumnDefinition Width="150*"/>
		</Grid.ColumnDefinitions>
		<ProgressBar x:Name="progressAll" Margin="2.191,2.192,0,1.301" Grid.Column="1" Grid.Row="2"/>
		<Label x:Name="txtProgress" Content="Label" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="40" Padding="0" Foreground="White" Grid.Column="1" Grid.Row="3"/>
		<Label x:Name="txtTitle" Content="Label" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="20" Padding="0" Foreground="White" Grid.Column="1" Grid.Row="1"/>
	</Grid>
</UserControl>